# API Keys
OPENAI_API_KEY=your_openai_api_key_here
SUPABASE_URL=your_supabase_url_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# OpenAI Configuration
# See latest model documentation: https://platform.openai.com/docs/models/gpt-4
OPENAI_MODEL_VERSION=gpt-4-turbo

# Environment
NODE_ENV=development
PORT=3001

# Debug Options
DEBUG=false

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key

# Example environment file - copy to .env and fill in the values
REACT_APP_OPENAI_API_KEY=your_openai_api_key

# Supabase credentials - Used for authentication and database
REACT_APP_SUPABASE_URL=your_supabase_url
REACT_APP_SUPABASE_KEY=your_supabase_anon_key

# Fix for webpack dev server initialization error
WDS_SOCKET_HOST=localhost
WDS_SOCKET_PORT=0 